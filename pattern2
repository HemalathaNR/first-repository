#include <stdio.h>

int main()
{
    int i,j,row,col;
    scanf("%d %d",&row,&col);
    for(i=1;i<=row;i++){
        if(i%2==0){
        for(j=1;j<=col;j++){
            if(j%2==0)
            printf("1");
            else
            printf("0");
        }
        }
        else{
            for(j=1;j<=col;j++){
            if(j%2==0)
            printf("0");
            else
            printf("1");
        }
        }
        printf("\n");
    }

    return 0;
}
